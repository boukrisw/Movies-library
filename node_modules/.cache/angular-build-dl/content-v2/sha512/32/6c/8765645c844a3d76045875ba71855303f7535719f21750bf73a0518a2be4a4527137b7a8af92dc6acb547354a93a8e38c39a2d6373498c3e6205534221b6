class FilmsComponent {
    constructor(tmdb, auth) {
        this.tmdb = tmdb;
        this.auth = auth;
        this.Fname = '';
        this.Lname = '';
        this.mail = '';
        this.pwd = '';
        this.birthday = '';
        // database = firebase.database;
        this.likes = firebase_app__WEBPACK_IMPORTED_MODULE_2__["database"]().ref('/likes/');
        this.dislikes = firebase_app__WEBPACK_IMPORTED_MODULE_2__["database"]().ref('/dislikes/');
        this.init();
    }
    gotDatalikes(data) {
        FilmsComponent.tabLikes = [];
        let films = data.val();
        if (films != null) {
            let keys = Object.keys(films);
            keys.map((elt, i) => {
                let k = keys[i];
                let key = Object.keys(films[k]);
                FilmsComponent.tabLikes.push({ id: elt, nb: key.length });
            });
        }
    }
    gotDatadislikes(data) {
        FilmsComponent.tabDisLikes = [];
        let films = data.val();
        if (films != null) {
            let keys = Object.keys(films);
            keys.map((elt, i) => {
                let k = keys[i];
                let key = Object.keys(films[k]);
                FilmsComponent.tabDisLikes.push({ id: elt, nb: key.length });
            });
        }
        //console.log('this.tabdisLikes',FilmsComponent.tabDisLikes[0]);
    }
    errorData(data) {
        console.log(data);
    }
    ngOnInit() {
    }
    init() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            yield this.tmdb.popularFilms();
            yield this.tmdb.upComingFilms();
            FilmsComponent.tabLikes = [];
            this.likes.on('value', this.gotDatalikes, this.errorData);
            this.dislikes.on('value', this.gotDatadislikes, this.errorData);
            this.Fname = '';
            this.Lname = '';
            this.mail = '';
            this.pwd = '';
            this.birthday = '';
        });
    }
    CreateAccount() {
        this.auth.signup(this.mail, this.pwd, this.Fname, this.Lname, this.birthday);
        this.Fname = '';
        this.Lname = '';
        this.mail = '';
        this.pwd = '';
        this.birthday = '';
        this.tmdb.RechercheTrouve = false;
        this.tmdb.RechrcheResult = null;
    }
    click(id) {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            this.tmdb.idFilm = id;
            this.tmdb.filmdetail = (yield this.tmdb.getMovie(id));
        });
    }
    like(id) {
        let like, unlike;
        //console.log('Like')
        if (this.auth.connected) {
            firebase_app__WEBPACK_IMPORTED_MODULE_2__["database"]().ref('/likes/' + id + '/' + this.auth.userId).once('value').then(function (snapshot) {
                like = snapshot.numChildren();
            }).then(() => {
                if (like == 1) {
                    firebase_app__WEBPACK_IMPORTED_MODULE_2__["database"]().ref('/likes/' + id + '/' + this.auth.userId).remove();
                    //console.log('remove like')
                }
                else if (like == 0) {
                    var currentdate = new Date();
                    let date = '';
                    if (currentdate.getDate() < 10)
                        date = date + '0';
                    date = date + currentdate.getDate() + "/";
                    if (currentdate.getMonth() + 1 < 10)
                        date = date + '0';
                    date = date + (currentdate.getMonth() + 1) + "/";
                    date = date + currentdate.getFullYear() + " ";
                    if (currentdate.getHours() < 10)
                        date = date + '0';
                    date = date + currentdate.getHours() + ":";
                    if (currentdate.getMinutes() < 10)
                        date = date + '0';
                    date = date + currentdate.getMinutes();
                    firebase_app__WEBPACK_IMPORTED_MODULE_2__["database"]().ref('likes/' + id + '/' + this.auth.userId).set({
                        date: date
                    });
                    //console.log('add like')
                }
            });
            firebase_app__WEBPACK_IMPORTED_MODULE_2__["database"]().ref('/dislikes/' + id + '/' + this.auth.userId).once('value').then(function (snapshot) {
                unlike = snapshot.numChildren();
            }).then(() => {
                if (unlike == 1) {
                    firebase_app__WEBPACK_IMPORTED_MODULE_2__["database"]().ref('/dislikes/' + id + '/' + this.auth.userId).remove();
                    //console.log('remove dislikes')
                }
            });
        }
    }
