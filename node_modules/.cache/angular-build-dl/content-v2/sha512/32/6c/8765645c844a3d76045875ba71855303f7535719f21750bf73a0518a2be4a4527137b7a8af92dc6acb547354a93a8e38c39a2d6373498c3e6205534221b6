class AuthenticationService {
    //listLikes //: FirebaseListObservable = [];
    constructor(afAuth, db) {
        this.afAuth = afAuth;
        this.db = db;
        this.connected = false;
        //this.userId.subscribe();
        //db.list<Object>('likes').valueChanges().subscribe(console.log);
    }
    signup(email, password, Fname, Lname, birthday) {
        this.afAuth.createUserWithEmailAndPassword(email, password)
            .then(value => {
            this.connected = true;
            this.userId = value.user.uid;
            /*Add user to DataBase! */
            firebase_app__WEBPACK_IMPORTED_MODULE_2__["database"]().ref('users/' + value.user.uid).set({
                email: email,
                mdp: password,
                nom: Lname,
                prenom: Fname,
                birthday: birthday
            });
        })
            .catch(err => {
            console.log('Something went wrong:', err.message);
        });
    }
    login(email, password) {
        this.afAuth.signInWithEmailAndPassword(email, password)
            .then(value => {
            this.connected = true;
            this.userId = value.user.uid;
            console.log('Nice, it worked!');
        })
            .catch(err => {
            console.log('Something went wrong:', err.message);
        });
    }
    logout() {
        this.afAuth.signOut();
        this.connected = false;
    }
}
